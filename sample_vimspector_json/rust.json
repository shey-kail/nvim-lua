{
  "configurations": {
    "cpp:launch": {
      "adapter": "CodeLLDB",
      "configuration": {
        "name": "rust",
        "type": "rustdbg",
        "request": "launch",
        "program": "${workspaceRoot}/target/debug/list",
        "args": ["*${ProgramArgs}"],
        "cwd": "${workspaceRoot}",
        "environment": [],
        "externalConsole": false,
        "stopAtEntry": true
        }
      }
    }
}


